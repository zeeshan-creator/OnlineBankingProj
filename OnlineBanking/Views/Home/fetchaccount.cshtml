@model IEnumerable<OnlineBanking.Models.customer>

@{
    ViewBag.Title = "fetchaccount"; 
    Layout = "~/Views/Layout/_weblayout.cshtml";
}

<h2>fetchaccount</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
    
        <th>
            @Html.DisplayNameFor(model => model.Account_No)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountType.Account_Type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
   
        <td>
            @Html.DisplayFor(modelItem => item.Account_No)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccountType.Account_Type)
        </td>
      
    </tr>
}

</table>
